using System.Diagnostics;

internal class Program
{
    static void Main(string[] args)
    {
        for (int i = 1; i < 13; i++)
        {
            Console.WriteLine("Tabla de multiplicar del {0}", i);
            for (int j = 1; j < 13; j++)
            {
                Console.WriteLine("{0} X {1} = {2}",i,j,(i*j));
            }
        }
    }
}